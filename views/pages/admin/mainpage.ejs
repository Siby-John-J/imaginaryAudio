
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Purple Admin</title>
    <!-- plugins:css -->
    <link rel="stylesheet" href="/assets2/vendors/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="/assets2/vendors/css/vendor.bundle.base.css">

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet" 
    integrity="sha384-9ndCyUaIbzAi2FUVXJi0CjmCapSmO7SnpJef0486qhLnuZ2cdeRhO02iuK6FUUVM" crossorigin="anonymous">

    <link rel="stylesheet" href="/assets2/css/style.css">
    <!-- End layout styles -->
    <link rel="shortcut icon" href="/assets2/images/favicon.ico" />
  </head>
  <body>
    <div class="container-scroller">
      <div class="container-fluid page-body-wrapper">
        <nav class="sidebar sidebar-offcanvas" id="sidebar">
          <ul class="nav">
            <li class="nav-item nav-profile">
              <a href="#" class="nav-link">
                <div class="nav-profile-image">
                  <img src="/assets2/images/faces/face1.jpg" alt="profile">
                  <span class="login-status online"></span>
                </div>
                <div class="nav-profile-text d-flex flex-column">
                  <span class="font-weight-bold mb-2">David Grey. Hs</span>
                  <span class="text-secondary text-small">Project Manager</span>
                </div>
                <i class="mdi mdi-bookmark-check text-success nav-profile-badge"></i>
              </a>
            </li>
            <li class="py-2 nav-item">
              <a class="nav-link" href="/admin/dashboard">
                <span class="menu-title">Dashboard</span>
                <i class="mdi mdi-home menu-icon"></i>
              </a>
            </li>
            <li class="py-2 nav-item">
              <a class="nav-link" href="/admin/customers">
                <span class="menu-title">Cutomers</span>
                <i class="mdi mdi-contacts menu-icon"></i>
              </a>
            </li>
            <li class="py-2 nav-item">
              <a class="nav-link" href="/admin/products">
                <span class="menu-title">Products</span>
                <i class="mdi mdi-format-list-bulleted menu-icon"></i>
              </a>
            </li>
            <li class="py-2 nav-item">
              <a class="nav-link" href="/admin/category">
                <span class="menu-title">Category</span>
                <i class="mdi mdi-chart-bar menu-icon"></i>
              </a>
            </li>
            <li class="py-2 nav-item">
              <a class="nav-link" href="/admin/orders">
                <span class="menu-title">Orders</span>
                <i class="mdi mdi-table-large menu-icon"></i>
              </a>
            </li>
            <li class="py-2 nav-item">
                <a class="nav-link" href="/admin/coupon/list">
                  <span class="menu-title">Coupon</span>
                  <i class="mdi mdi-table-large menu-icon"></i>
                </a>
              </li>
            </li>
            <li class="py-2 nav-item">
              <a class="nav-link" href="/admin/report">
                <span class="menu-title">Sales report</span>
                <i class="mdi mdi-table-large menu-icon"></i>
              </a>
            </li>
            <li class="py-2 nav-item">
              <a class="nav-link" href="/admin/banner">
                <span class="menu-title">Banners</span>
                <i class="mdi mdi-table-large menu-icon"></i>
              </a>
            </li>
            <li class="py-1 nav-item">
                <a class="nav-link" href="/admin/logout">
                  <span class="menu-title">Logout</span>
                  <i class="mdi mdi-table-large menu-icon"></i>
                </a>
              </li>
          </ul>
        </nav>
        <% if(page === "dashboard") { %>
          <div class="dashboard-page">
              <%- include('./dashboard') %>
          </div>
      <% } else if(page === "customers") { %>
          <div class="customers-page">
              <%- include('./customers') %>
          </div>
      <% } else if(page === "products") { %>
          <div class="products-page">
              <%- include('./products') %>
          </div>
      <% } else if(page === "category") { %>
          <div class="category-page">
              <%- include('./category') %>
          </div>
      <% } else if(page === 'orders') { %>
          <div class="orders-page">
              <%- include('./orders.ejs') %>
          </div>
      <% } else if(page === 'coupon') { %>
          <div class="coupon-page">
              <%- include('./category.ejs') %>
          </div>
      <% } else if(page === 'report') { %>
        <div class="report-page">
          <%- include('./report.ejs') %>
        </div>
      <% } else if(page === 'edit-category') { %>
        <div class="edit-cate-page">
          <%- include('./editCategory.ejs') %>
        </div>
      <% } else if(page === 'banner') { %>
        <div class="banner-page">
          <%- include('./banner.ejs') %>
        </div>
      <% } %>
      </div>
      
    </div>
    <script src="/assets2/vendors/js/vendor.bundle.base.js"></script>
    
    <script src="/assets2/vendors/chart.js/Chart.min.js"></script>
    <script src="/assets2/js/jquery.cookie.js" type="text/javascript"></script>

    <script src="/assets2/js/off-canvas.js"></script>
    <script src="/assets2/js/hoverable-collapse.js"></script>
    <script src="/assets2/js/misc.js"></script>
    
    <script src="/assets2/js/dashboard.js"></script>
    <script src="/assets2/js/todolist.js"></script>
    
    <!-- timer -->
    <script src="/js/other/timer.js"></script>
  </body>
</html>