                  <div class="row h-100 align-items-center">
                    <% for(let i of cate) { %>
                      <!-- <div><%= user %></div> -->
                      <a href="/<%= user %>/products?cate=<%= i.name %>" class="col-sm-6 col-md-4 col-xl mb-5 h-100">
                        <% if(i.image) { %>
                          <div class="card card-span h-100">
                            <% if(n >= 7) { %>
                              <% } %>
                              <img class="img-fluid h-100 rounded-circle" src="/img/cropped/category/<%= i.image %>" alt="..." />
                        <% } else { %>
                          <div class="card card-span h-100 rounded-circle"><img class="img-fluid rounded-circle h-100" src="/images/1-1.jpg" alt="..." />
                        <% } %>
                          <div class="card-body ps-0">
                            <h5 class="text-center fw-bold text-1000 text-truncate mb-2"><%= i.name %></h5>
                          </div>
                        </div>
                      </a>
                      <% n++ %>
                    <% } %>
